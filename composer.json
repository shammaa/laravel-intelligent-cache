{
    "name": "shammaa/laravel-intelligent-cache",
    "description": "An intelligent response caching package for Laravel with automatic invalidation and smart headers.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shammaa",
            "email": "contact@shammaa.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\IntelligentCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\IntelligentCache\\IntelligentCacheServiceProvider"
            ]
        }
    }
}